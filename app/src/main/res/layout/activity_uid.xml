<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:paddingHorizontal="30dp"
    tools:context=".UID">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:src="@drawable/derpaimon"
        android:contentDescription="@string/paimon_derp" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@drawable/rounded_corner"
        android:layout_below="@id/imageView"
        android:id="@+id/gi_fields">

        <androidx.cardview.widget.CardView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:id="@+id/gi"
            app:cardCornerRadius="8dp"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/gi48"
                android:importantForAccessibility="no" />
        </androidx.cardview.widget.CardView>

        <EditText
            android:id="@+id/uid_gi_text"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginHorizontal="15dp"
            android:layout_marginVertical="5dp"
            android:layout_toStartOf="@id/copyclear_uid_gi"
            android:layout_toEndOf="@id/gi"
            android:background="@null"
            android:hint="@string/type_uid"
            android:imeOptions="actionDone"
            android:importantForAutofill="no"
            android:inputType="number"
            android:singleLine="true"
            android:textSize="20sp"
            tools:ignore="VisualLintTextFieldSize,TextContrastCheck,DuplicateSpeakableTextCheck" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/copyclear_uid_gi">

            <ImageView
                android:id="@+id/copy_uid_gi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:importantForAccessibility="no"
                android:src="@drawable/baseline_content_copy_24"
                tools:ignore="ImageContrastCheck" />

            <ImageView
                android:id="@+id/clear_uid_gi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:importantForAccessibility="no"
                android:src="@drawable/baseline_cancel_24"
                tools:ignore="ImageContrastCheck" />

        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_corner"
        android:layout_below="@id/gi_fields"
        android:id="@+id/hsr_fields">

        <androidx.cardview.widget.CardView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:id="@+id/hsr"
            app:cardCornerRadius="8dp"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/hsr48"
                android:importantForAccessibility="no" />
        </androidx.cardview.widget.CardView>

        <EditText
            android:id="@+id/uid_hsr_text"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginHorizontal="15dp"
            android:layout_marginVertical="5dp"
            android:layout_toStartOf="@id/copyclear_uid_hsr"
            android:layout_toEndOf="@id/hsr"
            android:background="@null"
            android:hint="@string/type_uid"
            android:imeOptions="actionDone"
            android:importantForAutofill="no"
            android:inputType="number"
            android:singleLine="true"
            android:textSize="20sp"
            tools:ignore="VisualLintTextFieldSize,TextContrastCheck" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/copyclear_uid_hsr">

            <ImageView
                android:id="@+id/copy_uid_hsr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:importantForAccessibility="no"
                android:src="@drawable/baseline_content_copy_24"
                tools:ignore="ImageContrastCheck" />

            <ImageView
                android:id="@+id/clear_uid_hsr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:importantForAccessibility="no"
                android:src="@drawable/baseline_cancel_24"
                tools:ignore="ImageContrastCheck" />

        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_corner"
        android:layout_below="@id/hsr_fields"
        android:id="@+id/hi3_fields">

        <androidx.cardview.widget.CardView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:id="@+id/hi3"
            app:cardCornerRadius="8dp"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/hi348"
                android:importantForAccessibility="no" />
        </androidx.cardview.widget.CardView>

        <EditText
            android:id="@+id/uid_hi3_text"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginHorizontal="15dp"
            android:layout_marginVertical="5dp"
            android:layout_toStartOf="@id/copyclear_uid_hi3"
            android:layout_toEndOf="@id/hi3"
            android:background="@null"
            android:hint="@string/type_uid"
            android:imeOptions="actionDone"
            android:importantForAutofill="no"
            android:inputType="number"
            android:singleLine="true"
            android:textSize="20sp"
            tools:ignore="VisualLintTextFieldSize,TextContrastCheck" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/copyclear_uid_hi3">

            <ImageView
                android:id="@+id/copy_uid_hi3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:importantForAccessibility="no"
                android:src="@drawable/baseline_content_copy_24"
                tools:ignore="ImageContrastCheck" />

            <ImageView
                android:id="@+id/clear_uid_hi3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:importantForAccessibility="no"
                android:src="@drawable/baseline_cancel_24"
                tools:ignore="ImageContrastCheck" />

        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:background="@drawable/rounded_corner"
        android:layout_below="@id/hi3_fields"
        android:id="@+id/tot_fields">

        <androidx.cardview.widget.CardView
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:id="@+id/tot"
            app:cardCornerRadius="8dp"
            android:layout_marginStart="10dp"
            android:layout_centerVertical="true">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/tot48"
                android:importantForAccessibility="no" />
        </androidx.cardview.widget.CardView>

        <EditText
            android:id="@+id/uid_tot_text"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginHorizontal="15dp"
            android:layout_marginVertical="5dp"
            android:layout_toStartOf="@id/copyclear_uid_tot"
            android:layout_toEndOf="@id/tot"
            android:background="@null"
            android:hint="@string/type_uid"
            android:imeOptions="actionDone"
            android:importantForAutofill="no"
            android:inputType="number"
            android:singleLine="true"
            android:textSize="20sp"
            tools:ignore="VisualLintTextFieldSize,TextContrastCheck" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="10dp"
            android:id="@+id/copyclear_uid_tot">

            <ImageView
                android:id="@+id/copy_uid_tot"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:importantForAccessibility="no"
                android:src="@drawable/baseline_content_copy_24"
                tools:ignore="ImageContrastCheck" />

            <ImageView
                android:id="@+id/clear_uid_tot"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:importantForAccessibility="no"
                android:src="@drawable/baseline_cancel_24"
                tools:ignore="ImageContrastCheck" />

        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@id/tot_fields"
        android:background="@drawable/rounded_corner3"
        android:gravity="center"
        android:padding="8dp">

        <ImageView
            android:id="@+id/power_off"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:importantForAccessibility="no"
            android:src="@drawable/baseline_arrow_back3_24"
            tools:ignore="ImageContrastCheck" />

        <TextView
            android:id="@+id/button_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginHorizontal="2dp"
            android:layout_toEndOf="@id/power_off"
            android:background="@null"
            android:text="@string/return_home"
            android:textColor="@color/white"
            android:textSize="22sp"
            android:textStyle="bold"
            tools:ignore="TextContrastCheck" />

    </RelativeLayout>

</RelativeLayout>